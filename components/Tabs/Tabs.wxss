.tabs {
  width: 100%;
}
.tabs .tabs_title {
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabs .tabs_title .tabs_item {
  flex: 1;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabs .tabs_title .isActive {
  border-bottom: 5rpx solid var(--themeColor);
}
