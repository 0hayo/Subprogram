.swiper swiper-item image {
  width: 100%;
}
.cates {
  display: flex;
  align-items: center;
}
.cates navigator {
  flex: 1;
  padding: 20rpx;
}
.cates navigator image {
  width: 100%;
}
.floor .floor_list .floor_list_title {
  padding: 20rpx 0 0 0;
}
.floor .floor_list .floor_list_title image {
  width: 100%;
}
.floor .floor_list .floor_list_main {
  overflow: hidden;
}
.floor .floor_list .floor_list_main .floor_list_list {
  width: 33.33%;
  float: left;
}
.floor .floor_list .floor_list_main .floor_list_list image {
  width: 100%;
  height: 100%;
}
.floor .floor_list .floor_list_main .floor_list_list:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor .floor_list .floor_list_main .floor_list_list:nth-child(2),
.floor .floor_list .floor_list_main .floor_list_list:nth-child(3),
.floor .floor_list .floor_list_main .floor_list_list:nth-child(4),
.floor .floor_list .floor_list_main .floor_list_list:nth-child(5) {
  border-bottom: 10rpx solid #fff;
}
.floor .floor_list .floor_list_main .floor_list_list:nth-child(3),
.floor .floor_list .floor_list_main .floor_list_list:nth-child(5) {
  border-right: 10rpx solid #fff;
}
.floor .floor_list .floor_list_main .floor_list_list:nth-child(1) {
  border-left: 10rpx solid #fff;
}
